<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui">

    
    <ui:with field="res"
		type="fr.tsadeo.app.japicgwtp.client.resources.AppResources" />
    

	<b:Row>
		<b:Column size="MD_2">
			<b:Label ui:field="lbTitle" addStyleNames="{res.style.appTypeLabel}" type="WARNING" />
		</b:Column>
		<b:Column size="MD_10">
			<b.html:Paragraph ui:field="lbMessage" />
		</b:Column>

	</b:Row>
</ui:UiBinder>
